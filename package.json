{
  "name": "fast-json",
  "description": "A lightning fast on the fly JSON parser",
  "version": "1.1.0",
  "author": "Alejandro Santiago Nieto",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "mocha test/*.js",
    "test:cover": "istanbul cover _mocha test/*.js",
    "lint": "eslint index.js lib/*.js test/*.js example/*.js",
    "lint:fix": "eslint index.js lib/*.js test/*.js example/*.js --fix",
    "check": "npm test && npm run lint && npm outdated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alemures/fast-json.git"
  }
}
